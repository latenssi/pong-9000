#pragma strict
var prefabBlock : Transform;
var repeatTime : int;
private var nextBlock : int;

function Start () {

}

function Update () {
if (Time.time > nextBlock)
	{
	nextBlock = Time.time + repeatTime;
	// CREATE BLOCK AT POSITION
	Instantiate(prefabBlock, gameObject.transform.position, Quaternion.identity);
	}
}
